{
    "Version": "2012-10-17",
    "Statement": [
    {
        "Effect": "Allow",
        "Action": [
        "ec2:DescribeInstances",
        "ec2:DescribeSecurityGroups",
        "ec2:CreateTags",
        "ec2:RunInstances",
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "iam:GetUser",
        "iam:CreateRole"
        ],
        "Resource": "*"
    }
    ]
}